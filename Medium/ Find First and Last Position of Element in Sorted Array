func searchRange(nums []int, target int) []int {
    first, last := 0, len(nums) - 1

    res := []int{-1,-1}

    for last >= first{
        mid := first +(last - first)/2
        if nums[mid] == target{
            res[0] = mid
            last = mid - 1
        }else if nums[mid] > target{
            last = mid - 1
        }else{
            first = mid + 1
        }
    }

    first, last = 0, len(nums) -1
    for last >= first{
        mid := first +(last - first)/2
        if nums[mid] == target{
            res[1] = mid
            first = mid + 1
        }else if nums[mid] > target{
            last = mid - 1
        }else{
            first = mid + 1
        }
    }
    return res
}

