func lengthOfLongestSubstring(s string) int {
	freq := make(map[byte]int)
	l := 0
	res := 0

	for r := 0; r < len(s); r++ {
		if start, ok := freq[s[r]]; ok {
			l = max(l, start)
		}

		res = max(res, r-l+1)

		freq[s[r]] = r + 1
	}
	return res
}

func max(n, m int) int {
	if n > m {
		return n
	}
	return m
}
